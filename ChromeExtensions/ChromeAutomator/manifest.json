{
  "manifest_version": 2,
  "name": "NNG Experiments",
  "description": "This extension records and replay user activity",
  "version": "1.0",

  "browser_action": {
	"default_title":"",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "permissions": [
    "alarms",
	"tabs",
	"activeTab",
	"webNavigation",
    "*://*/*"
  ]
}
